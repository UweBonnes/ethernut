/******************************************************************************
 *
 *  stm32F03Xx4_flash.ld - linker script for nutos on cortex-m0 STM32F03Xx4
 *
 *****************************************************************************/

ENTRY(NutInit)
SEARCH_DIR(.)

MEMORY
{
    FLASH0 (rx) : ORIGIN = 0x08000000, LENGTH = 16K
    SRAM0 (rwx) : ORIGIN = 0x20000000, LENGTH =  4K
}

INCLUDE stm32f10x_flash.ld
